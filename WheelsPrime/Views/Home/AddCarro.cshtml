@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_OficinaPartial.cshtml";
    
}



<div class="wellback col-lg-12">

    <h1>Adicionar Carro </h1>


    <div class="col-lg-8 ">
        <form class="form-horizontal">
            <div class="form-group">
                <label for="inputMatricula" class="col-sm-2 control-label">Matricula :</label>
                <div class="col-sm-10">
                    <input type="" class="form-control" id="inputMatricula" placeholder="Matricula">
                </div>
            </div>

            <div class="form-group">
                <label for="inputMarca" class="col-sm-2 control-label">Marca :</label>
                <div class="col-sm-10">
                    <input type="" class="form-control" id="inputMarca" placeholder="Marca">
                </div>
            </div>

            <div class="form-group">
                <label for="inputModelo" class="col-sm-2 control-label">Modelo :</label>
                <div class="col-sm-10">
                    <input type="" class="form-control" id="inputModelo" placeholder="Modelo">
                </div>
            </div>

            <div class="form-group">
                <label for="inputTipo" class="col-sm-2 control-label">Tipo :</label>
                <div class="col-sm-10">
                    <input type="" class="form-control" id="inputTipo" placeholder="Tipo">
                </div>
            </div>

            <div class="form-group">
                <label for="" class="col-sm-2 control-label">Imagem :</label>
                <div class="col-sm-10">
                    <input type="" class="form-control" id="inputTipo" placeholder="URL or Destination">
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-10 pull-right">
                    <button type="button" class="btn btn-primary">Upload</button>
                    <button type="button" class="btn btn-default">URL Code</button>
                </div>
            </div>

    </div>

    <div class="col-lg-4">
        <img src="data:image/svg+xml;base64," alt="..." class="img-rounded">
    </div>


</div>

