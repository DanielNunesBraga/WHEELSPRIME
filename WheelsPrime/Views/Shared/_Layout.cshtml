<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/Theme.css")
    @Styles.Render("~/Content/style.css")
    @Styles.Render("~/Content/animate.css")
    @Styles.Render("~/Content/ionicons.min.css")
    @Styles.Render("~/Content/owl.carousel.css")
    @Styles.Render("~/Content/owl.theme.css")
    @Styles.Render("~/Content/font-awesome.css")
    @Styles.Render("~/Content/font-awesome.min.css")
    @Styles.Render("~/Content/footer-distributed-with-address-and-phones.css")

    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Arimo:300,400,700,400italic,700italic'>
    <link href='http://fonts.googleapis.com/css?family=Oswald:400,300,700' rel='stylesheet' type='text/css'>
</head>
<body>
        <div class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a rel="home" href="@Url.Action("Index", "Home")">
                        <img src="http://i.imgur.com/nleWvkE.jpg?1" class="img-responsive">
                    </a>
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li>@Html.ActionLink("Stand", "Stand", "Stand")</li>

                        <li>@Html.ActionLink("Oficina", "out", "Home")</li>

                    </ul>
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </div>

    <div class="container body-content">
        @RenderBody()

    </div>

    <footer class="footer-distributed">
        <div>

            <div class="row margin-bottom-60">
                <div class="col-sm-4 margin-bottom-30">
                    <div class="footer-inner">
                        <h3><a href="index.html"><img src="http://i.imgur.com/nleWvkE.jpg?1" alt=""></a> </h3>
                        <p>
                            O nosso site é o mais tops !
                        </p>
                        <div class="divided-30"></div>
                        <ul class="social list-inline">
                            <li><a href="http://www.facebook.com" class="fb"><i class="ion-social-facebook"></i></a></li>
                            <li><a href="#" class="dribbble"><i class="ion-social-instagram"></i></a></li>
                            <li><a href="#" class="in"><i class="ion-social-linkedin"></i></a></li>
                            <li><a href="#" class="twitter"><i class="ion-social-googleplus-outline"></i></a></li>
                        </ul>
                    </div><!--fotter inner-->
                </div>

                <div class="col-sm-4 margin-bottom-30">
                    <div class="footer-inner">
                        <h3>Contact us</h3>

                        <ul class="list-unstyled contact-list">
                            <li><i class="ion-ios-location">  R. Cmte. Pinho e Freitas, 3750-127 Águeda</i></li>
                            <li><i class="ion-ios-telephone">  234 500 611</i></li>
                            <li><i class="ion-ios-email">  RH@wheelsprime.com</i></li>
                        </ul>
                    </div><!--fotter inner-->
                </div>
                <div class="col-sm-4">
                    <div class="footer-inner">
                        <h3>Links</h3>
                        <ul class="list-unstyled contact-list">
                            <li class="ion-home">  @Html.ActionLink("Home", "Index", "Home")</li>
                            <li class="ion-android-car">  @Html.ActionLink("Vehicles", "Index", "Home")</li>
                            <li class="ion-ios-contact-outline">  @Html.ActionLink("Client Area", "Index", "Home")</li>
                            <li class="ion-wrench">  @Html.ActionLink("Services", "Index", "Home")</li>
                            <li class="ion-information-circled">  @Html.ActionLink("About", "Index", "Home")</li>
                        </ul>
                    </div><!--fotter inner-->
                </div>
            </div>
            <hr class="hr"/>
        </div>
            <p class="text-center">©Copyright 2015. All rights reserved.</p>
            <p class="text-center">WheelsPrime</p>
    </footer>
    
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")

        @RenderSection("scripts", required: false)
</body>
</html>

   